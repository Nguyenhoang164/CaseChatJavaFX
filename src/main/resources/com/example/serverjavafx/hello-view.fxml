<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.serverjavafx.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Label fx:id="Message" alignment="TOP_CENTER" prefWidth="200.0" prefHeight="300.0"/>
  <HBox spacing="10.0">
    <TextField fx:id="Input_Message" prefWidth="400.0"/>
    <Button  text="Add" onAction="#AddMessage" alignment="BOTTOM_CENTER" />

  </HBox>
</VBox>
